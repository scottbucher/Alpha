{
    "name": "my-bot",
    "version": "1.0.0",
    "author": "Kevin Novak",
    "description": "A TypeScript Discord bot with multi-machine sharding and bot-site support.",
    "homepage": "https://github.com/KevinNovak/Discord-TypeScript-Sharding-Template#readme",
    "license": "MIT",
    "engines": {
        "node": ">=12.14.0"
    },
    "main": "dist/start.js",
    "scripts": {
        "start": "tsc && node dist/start.js",
        "start-shard": "tsc && node dist/app.js",
        "start-pm2": "tsc && pm2 start process.json",
        "stop-pm2": "pm2 stop process.json",
        "delete-pm2": "pm2 delete process.json",
        "build": "tsc"
    },
    "dependencies": {
        "discord.js": "12.2.0",
        "emoji-regex": "9.0.0",
        "moment": "2.26.0",
        "mysql": "2.18.1",
        "node-schedule": "1.3.2",
        "pm2": "^4.4.0",
        "request": "2.88.2",
        "request-promise": "4.2.5"
    },
    "devDependencies": {
        "@types/moment-timezone": "0.5.13",
        "@types/mysql": "2.15.13",
        "@types/node": "14.0.11",
        "@types/request": "2.48.5",
        "@types/request-promise": "4.1.46",
        "tslint": "^6.1.2",
        "typescript": "^3.9.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KevinNovak/Discord-TypeScript-Sharding-Template.git"
    },
    "bugs": {
        "url": "https://github.com/KevinNovak/Discord-TypeScript-Sharding-Template/issues"
    }
}
